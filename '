#include <stdio.h>

void for_fun() {
    for (int i = 0; i < 9; ++i) {
        printf("Having fun %d\n", i);
    }
}

int main() {
    printf("Hello friend, hello friend.\n");

    int positions[2] = {12, 15};
    printf("X position: %d\n", positions[0]);
    printf("Y position: %d\n", positions[1]);

    return 0;
}
